<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eku.intents">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />


    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@drawable/ic_launcher" android:theme="@style/AppTheme">
      <activity android:name=".intent">
          <intent-filter>
              <action android:name="android.intent.action.MAIN"></action>
              <category android:name="android.intent.category.LAUNCHER"></category>
          </intent-filter>
      </activity>
        <activity android:name=".app_act"></activity>
        <activity android:name=".setting_code"></activity>
        <activity android:name=".wifi_main"></activity>
        <activity android:name=".wifi_main2"></activity>
    </application>

</manifest>
